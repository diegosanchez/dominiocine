<a href="/"><img src="/images/logodomino.png" alt="logo"></a>

<ul>
{{#each movies}}
  <li class="itemFilm">
	<a href="#/movie/{{this.title}}">{{title}}</a>
	
	<div class="images">
		<ul>
		{{#each this.images}}
			<li>
		      <img src="{{this.unescapedUrl}}" alt="{{this.title}}" />
		   </li>
		{{/each}}
		</ul>
		
	</div>
	
	<div class="books">
		{{#each this.books}}
		  {{this.id}}
		{{/each}}
		
	</div>
	
  </li>
 {{/each}}
 
 </ul>